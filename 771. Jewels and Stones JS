var numJewelsInStones = function(jewels, stones) {
    let count = 0 

    // Time Complexity: O(m * n) Quadratic Time
    // Space Complexity: O(1) Constant Space
    for (let i = 0; i < stones.length; i++){
        for(let j = 0; j < jewels.length; j++){
            if(stones[i] === jewels[j]){
                        count++;
                        break;
            }
        }
    }
    return count
};
